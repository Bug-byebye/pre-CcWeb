her' });\r\n      },\r\n      // 跳转到其他服务页面（目前是占位用）\r\n      goToService(serviceId) {\r\n        console.log(`跳转到服务 ${serviceId}`);\r\n        // 这里你可以根据不同的 serviceId 跳转到不同的页面\r\n        // 例如：this.$router.push({ name: 'servicePage', params: { id: serviceId } });\r\n      }\r\n    }\r\n  };\r\n  </script>\r\n  \r\n  <style scoped>\r\n  /* 容器整体布局 */\r\n  .container {\r\n    display: flex;\r\n    flex-direction: column;\r\n    height: 100vh;\r\n  }\r\n  \r\n  /* 顶部导航栏样式 */\r\n  .header {\r\n    display: flex;\r\n    justify-content: space-between;\r\n    align-items: center;\r\n    background-color: #4CAF50;\r\n    padding: 15px 30px;\r\n    color: white;\r\n  }\r\n  \r\n  .logo {\r\n    display: flex;\r\n    align-items: center;\r\n  }\r\n  \r\n  .logo-icon {\r\n    width: 40px;\r\n    height: 40px;\r\n    margin-right: 15px;\r\n  }\r\n  \r\n  .site-info {\r\n    display: flex;\r\n    flex-direction: column;\r\n  }\r\n  \r\n  .site-name {\r\n    font-size: 24px;\r\n    font-weight: bold;\r\n    margin: 0;\r\n  }\r\n  \r\n  .site-tagline {\r\n    font-size: 14px;\r\n    margin: 0;\r\n  }\r\n  \r\n  .user-info {\r\n    display: flex;\r\n    align-items: center;\r\n  }\r\n  \r\n  .user-avatar {\r\n    width: 40px;\r\n    height: 40px;\r\n    border-radius: 50%;\r\n    margin-right: 10px;\r\n  }\r\n  \r\n  .username {\r\n    font-size: 16px;\r\n  }\r\n  \r\n  /* 中间的可选服务并排显示 */\r\n  .services {\r\n    display: flex;\r\n    justify-content: space-around;\r\n    padding: 20px 0;\r\n  }\r\n  \r\n  .services button {\r\n    padding: 10px 20px;\r\n    font-size: 16px;\r\n    background-color: #008CBA;\r\n    color: white;\r\n    border: none;\r\n    border-radius: 5px;\r\n    cursor: pointer;\r\n    transition: background-color 0.3s ease;\r\n  }\r\n  \r\n  .services button:hover {\r\n    background-color: #007B9E;\r\n  }\r\n  \r\n  /* 底部信息样式 */\r\n  .footer {\r\n    background-color: #333;\r\n    color: white;\r\n    text-align: center;\r\n    padding: 10px;\r\n  }\r\n  </style